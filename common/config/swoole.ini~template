[http]
server_host = 0.0.0.0 ;监听的ip地址,默认0.0.0.0监听全部地址
server_port = 19501 ;监听的端口,默认9501
worker_num = 16 ;进程数
reactor_num = 16 ;线程数,默认会启用CPU核数相同的数量,一般设置为CPU核数的1-4倍
backlog = 128 ;Listen队列长度
;max_request = 1000 ;进程的最大任务数
daemonize = 0 ;护进程化,后台运行 ,默认前台运行
log_file = logs/http.log ;swoole错误日志文件: #-Master进程,$-Manager进程,*-Worker进程,^-Task进程
log_level = 0 ;错误日志打印的等级，范围是0-5:0 =>DEBUG,1 =>TRACE,2 =>INFO,3 =>NOTICE,4 =>WARNING,5 =>ERROR
